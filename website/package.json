{
  "name": "design-system.sf.gov",
  "private": true,
  "scripts": {
    "build": "wireit",
    "clear": "docusaurus clear",
    "lint": "wireit",
    "serve": "docusaurus serve",
    "start": "wireit",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "wireit": {
    "build": {
      "command": "docusaurus build --out-dir public",
      "files": [
        "*.js",
        "docs/**/*",
        "src/**/*",
        "static/**/*"
      ],
      "output": [
        "public/**/*"
      ],
      "dependencies": [
        "../packages/sfgov-design-system:build"
      ]
    },
    "start": {
      "command": "docusaurus start",
      "dependencies": [
        "../packages/sfgov-design-system:build"
      ]
    },
    "lint": {
      "command": "eslint --color --cache --cache-location=.eslintcache src lib *.js",
      "files": [
        "src/**/*.{js,jsx}",
        "lin/**/*.js",
        "*.js"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.17.12",
    "@docusaurus/core": "^2.0.0-rc.1",
    "@docusaurus/preset-classic": "^2.0.0-rc.1",
    "@docusaurus/theme-live-codeblock": "^2.0.0-rc.1",
    "@mdx-js/react": "^1.6.22",
    "@types/node": "^16.11.32",
    "autoprefixer": "^10.4.5",
    "clsx": "^1.1.1",
    "filesize": "^8.0.7",
    "postcss": "^8.4.12",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-masonry-css": "^1.0.16",
    "sfgov-design-system": "2.4.0",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@docusaurus/eslint-plugin": "^2.0.0-rc.1",
    "@types/eslint": "^8.4.2",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "prop-types": "^15.8.1"
  }
}
