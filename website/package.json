{
  "name": "design-system.sf.gov",
  "private": true,
  "scripts": {
    "build": "docusaurus build --out-dir public",
    "clear": "docusaurus clear",
    "lint": "wireit",
    "serve": "docusaurus serve --dir public",
    "start": "wireit",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-translations": "docusaurus write-translations"
  },
  "wireit": {
    "start": {
      "command": "docusaurus start",
      "dependencies": [
        "../packages/sfgov-design-system:build"
      ]
    },
    "lint": {
      "command": "eslint --color --cache --cache-location=.eslintcache src lib *.js",
      "files": [
        "src/**/*.{js,jsx}",
        "lib/**/*.js",
        "*.js"
      ],
      "output": []
    }
  },
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@docusaurus/core": "^2.0.0-rc.1",
    "@docusaurus/eslint-plugin": "^2.0.0-rc.1",
    "@docusaurus/plugin-client-redirects": "^2.0.0-rc.1",
    "@docusaurus/preset-classic": "^2.0.0-rc.1",
    "@docusaurus/theme-live-codeblock": "^2.0.0-rc.1",
    "@mdx-js/react": "^1.6.22",
    "@types/eslint": "^8.4.2",
    "@types/node": "^16.11.32",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "clsx": "^1.1.1",
    "eslint-plugin-react": "^7.30.0",
    "filesize": "^8.0.7",
    "postcss": "^8.4.14",
    "prism-react-renderer": "^1.3.1",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^17.0.2",
    "react-masonry-css": "^1.0.16",
    "sfgov-design-system": "2.3.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.4",
    "use-bus": "^2.5.1"
  }
}