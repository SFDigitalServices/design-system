{
  "name": "sfgov-design-system",
  "version": "2.4.0",
  "repository": "SFDigitalServices/design-system",
  "author": "City & County of San Francisco, California",
  "license": "MIT",
  "browser": "dist/js/sfds.js",
  "module": "dist/js/sfds.mjs",
  "files": [
    "dist",
    "docs",
    "src",
    "tailwind.config.js"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "run-s build-assets build-docs",
    "build-assets": "run-p build-css build-js build-icons",
    "build-icons": "scripts/build-icons.js",
    "build-docs": "NODE_ENV=production eleventy",
    "build-css": "postcss --verbose -d dist/css src/css/*.css",
    "build-js": "rollup -c",
    "build-clean": "run-s clean build",
    "develop": "NODE_ENV=development run-p start watch",
    "clean": "rm -rf dist",
    "lint": "run-p lint-js lint-docs lint-scripts",
    "lint-css": "stylelint src/css",
    "lint-docs": "markdownlint --rules './lib/markdownlint/*.js' 'docs/**/*.md'",
    "lint-js": "eslint src docs lib '*.js' 'scripts/*.js'",
    "lint-scripts": "shellcheck scripts/*.sh",
    "prepublishOnly": "scripts/prepublish-checks.sh",
    "watch": "run-p watch-css watch-js watch-icons",
    "watch-css": "watchy -w '*.config.js' -w src -- npm run build-css",
    "watch-icons": "watchy -w src/icons -- npm run build-icons",
    "watch-js": "npm run build-js -- --watch",
    "watch-lint": "watchy -w '{lib,src,scripts,docs}' -- npm run lint",
    "start": "eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@fec/eleventy-plugin-remark": "^2.2.1",
    "@github/clipboard-copy-element": "^1.1.2",
    "@github/details-dialog-element": "^3.0.4",
    "@github/hotkey": "^1.6.0",
    "@octokit/rest": "^18.12.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "autoprefixer": "^10.2.6",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.3.1",
    "cssnano": "^5.0.6",
    "debounce": "^1.2.1",
    "delegated-events": "^1.1.2",
    "dotenv": "^10.0.0",
    "eslint": "^7.17.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-sfgov": "^0.0.0-da6c354",
    "eslint-plugin-unicorn": "^39.0.0",
    "execa": "^5.0.0",
    "fs-extra": "^10.0.0",
    "globby": "^5.0.0",
    "hast-util-to-html": "^7.1.3",
    "hastscript": "^6.0.0",
    "js-yaml": "^3.14.1",
    "lodash.merge": "^4.6.2",
    "markdownlint-cli": "^0.30.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.3",
    "outdent": "^0.8.0",
    "postcss": "^8.4.5",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "postcss-normalize-charset": "^5.0.1",
    "prettysize": "^2.0.0",
    "rehype-raw": "^5.1.0",
    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-autolink-headings": "^6.0.1",
    "remark-behead": "^2.3.3",
    "remark-directive": "^1.0.1",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-rehype": "^8.1.0",
    "remark-slug": "^6.0.0",
    "rollup": "^2.50.2",
    "rollup-plugin-terser": "^7.0.2",
    "selector-observer": "^2.1.6",
    "semver": "^7.3.5",
    "shellcheck": "^1.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "tailwindcss": "^2.0.2",
    "tailwindcss-interaction-variants": "^5.0.0",
    "unist-util-select": "^3.0.4",
    "unist-util-visit": "^2.0.3",
    "watchy": "^0.9.9"
  }
}
