{
  "name": "@sfgov/icons",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist",
    "generated",
    "src"
  ],
  "scripts": {
    "fetch": "scripts/fetch.sh",
    "postfetch": "eslint --fix generated/jsx --ext js,jsx",
    "build": "wireit",
    "build:js": "wireit",
    "lint": "eslint --ext js,jsx,mjs,ts,tsx .",
    "prebuild": "rm -rf dist",
    "postbuild": "du -h dist/*.*js"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:js"
      ]
    },
    "build:js": {
      "command": "rollup -c",
      "dependencies": [],
      "files": [
        "src/**/*",
        "*.config.*js"
      ],
      "outputs": [
        "dist/*.*js"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@figma-export/cli": "^4.5.0",
    "@figma-export/output-components-as-svg": "^4.5.0",
    "@figma-export/output-components-as-svgr": "^4.5.0",
    "@figma-export/transform-svg-with-svgo": "^4.5.0",
    "@figma-export/types": "^4.5.0",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@sfgov/react": "0.0.1",
    "@stitches/react": "^1.2.8",
    "core-js": "^3.26.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.78.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}